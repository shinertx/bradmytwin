NODE_ENV=development

# Core
DATABASE_URL=postgres://postgres:postgres@postgres:5432/brad
REDIS_URL=redis://redis:6379
JWT_SECRET=replace-with-long-random-secret
APP_BASE_URL=https://api.brad.com
WEB_BASE_URL=https://brad.com
API_BASE_URL=https://api.brad.com
BETA_ALLOW_UNVERIFIED_WEB=false
BETA_STRICT_APPROVALS=true
BETA_KILL_SWITCH_WRITES=false

# OpenClaw
OPENCLAW_MODE=stub
OPENCLAW_URL=https://openclaw.internal
OPENCLAW_API_KEY=
OPENCLAW_CLI_BIN=openclaw
OPENCLAW_CLI_AGENT_ID=brad-runtime
OPENCLAW_CLI_TIMEOUT_MS=90000
OPENCLAW_MODEL_DEFAULT=gpt-4.1
OPENCLAW_MODEL_TEMPERATURE=0.2

# Twilio SMS + WhatsApp
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_SMS_FROM=+15550001111
TWILIO_WHATSAPP_FROM=whatsapp:+14155238886

# Meta WhatsApp Cloud API
META_WHATSAPP_ACCESS_TOKEN=
META_WHATSAPP_PHONE_NUMBER_ID=
META_WHATSAPP_WABA_ID=
META_GRAPH_API_VERSION=v22.0
META_WEBHOOK_VERIFY_TOKEN=
META_APP_SECRET=

# Telegram
TELEGRAM_BOT_TOKEN=
TELEGRAM_WEBHOOK_SECRET=

# OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_LOGIN_REDIRECT_URI=https://api.brad.com/auth/login/google/callback
GOOGLE_OAUTH_REDIRECT_URI=https://api.brad.com/auth/link/google/callback

# GCP KMS (optional in local dev)
KMS_KEY_NAME=projects/PROJECT_ID/locations/global/keyRings/brad/cryptoKeys/connector-tokens

# Browser safety
BROWSER_ALLOWLIST=example.com,forms.gle,docs.google.com
